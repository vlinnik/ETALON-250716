    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Simulator",
            "type": "debugpy",
            "request": "launch",
            "module": "gui",
            "args": ["--simulator"],
            "env" : {"PYTHONPYCACHEPREFIX":"${workspaceFolder}/__pycache__"}
        },
        {
            "name": "Run Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env" : {"PYTHONPYCACHEPREFIX":"${workspaceFolder}/__pycache__"}
        }
    ]
}